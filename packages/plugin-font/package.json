{
    "name": "@embedpdf/plugin-font",
    "version": "0.0.1",
    "description": "Font plugin for EmbedPDF - enables custom font loading via Local Font Access API",
    "type": "module",
    "license": "MIT",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        },
        "./vue": {
            "types": "./dist/vue/index.d.ts",
            "import": "./dist/vue/index.js",
            "require": "./dist/vue/index.cjs"
        }
    },
    "scripts": {
        "build:base": "vite build --mode base",
        "build:vue": "vite build --mode vue",
        "build": "pnpm run clean && concurrently -c auto -n base,vue \"vite build --mode base\" \"vite build --mode vue\"",
        "clean": "rimraf dist"
    },
    "dependencies": {
        "@embedpdf/models": "workspace:*",
        "@embedpdf/utils": "workspace:*"
    },
    "devDependencies": {
        "@embedpdf/build": "workspace:*",
        "@types/react": "^18.2.0",
        "typescript": "^5.0.0"
    },
    "peerDependencies": {
        "@embedpdf/core": "workspace:*",
        "vue": ">=3.2.0"
    },
    "files": [
        "dist",
        "README.md"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/embedpdf/embed-pdf-viewer",
        "directory": "packages/plugin-font"
    },
    "homepage": "https://www.embedpdf.com/docs",
    "bugs": {
        "url": "https://github.com/embedpdf/embed-pdf-viewer/issues"
    },
    "publishConfig": {
        "access": "public"
    }
}